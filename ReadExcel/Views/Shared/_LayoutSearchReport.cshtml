<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Content/upload.css" type='text/css' rel="stylesheet" />
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/jquery.dataTables.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/jquery-ui.js"></script>
    


</head>
<body>
    <div class="navbar navbar-light navbar-fixed-top" style="background-color: #e3f2fd">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*<span class="navbar-brand"><a href="/MTDashBoard/Home/Index">MT Dashboard</a></span>*@
              
            </div>
          
            <ul class="nav navbar-nav">

                <li>
                    <a href="/MTDashBoard/Home/SearchSellIn">Hiển thị sell in</a>
                    @*@Html.ActionLink("Search sell in", "SearchSellIn", "Home")*@
                </li>

                <li>
                    <a href="/MTDashBoard/Home/SearchSellOut">Hiển thị sell out</a>
                    @*@Html.ActionLink("Search sell out", "SearchSellOut", "Home")*@
                </li>
                <li>
                    <a href="/MTDashBoard/Home/SearchWholeSaler">Hiển thị sản phẩm áp dụng cho Chuỗi khách hàng</a>
                    @*@Html.ActionLink("Search sell out", "SearchSellOut", "Home")*@
                </li>
            </ul>
                @*@Html.Partial("_LoginPartial")*@
            
        </div>
    </div>
    <div class="container body-content" style="margin-top:75px">
        @RenderBody()
        <hr />
        <footer>
            @*<p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>*@
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
